{
  "version": 2,
  "builds": [
    {
      "src": "src/entry-points/vercel.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/entry-points/vercel.ts"
    }
  ],
  "env": {
    "GOOGLE_APPLICATION_CREDENTIALS": "@google_application_credentials"
  }
}
